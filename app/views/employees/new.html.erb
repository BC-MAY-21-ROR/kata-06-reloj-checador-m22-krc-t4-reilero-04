<h1>Create new employee</h1>
<%= form_with model: @employee do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div>
    <%= form.label :position %>
    <%= form.text_field :position %>
  </div>
  <div>
    <%= form.label :private_number %>
    <%= form.text_field :private_number %>
  </div>
  <div>
    <%= form.label 'Rol (Admin?)' %>
    <%= form.text_field :admin %>
  </div>
  <div>
    <%= form.label 'Branch id assigned' %>
    <%= form.number_field :store_id %>
  </div>
  <div>
    <%= form.submit 'Create new employee' %>
  </div>
<% end %>
<%= render 'back_to_employee' %>
