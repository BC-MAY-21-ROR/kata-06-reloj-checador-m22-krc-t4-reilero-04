<div class="gray-panel">
  <h1>Edit Stores</h1>
  <%= form_with model: @store do |form| %>
    <div>
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>
    <div>
      <%= form.label :address %>
      <%= form.text_field :address %>
    </div>
    <%= form.submit 'Edit store' %>
  <% end %>
  <%= render 'back_to_stores' %>
</div>
